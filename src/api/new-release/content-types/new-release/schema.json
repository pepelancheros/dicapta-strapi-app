{
  "kind": "collectionType",
  "collectionName": "new_releases",
  "info": {
    "singularName": "new-release",
    "pluralName": "new-releases",
    "displayName": "New release",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "accessibilityReleaseYear": {
      "type": "biginteger",
      "required": true,
      "min": "2020",
      "max": "2220"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "series": {
      "type": "boolean",
      "required": true
    },
    "seasonNumber": {
      "type": "integer",
      "required": false
    },
    "language": {
      "type": "enumeration",
      "enum": [
        "SP",
        "EN"
      ],
      "required": true
    },
    "CC": {
      "type": "boolean",
      "required": true
    },
    "AD": {
      "type": "boolean",
      "required": true
    },
    "ASL": {
      "type": "boolean",
      "required": true
    },
    "provider": {
      "type": "string"
    },
    "whereToWatch": {
      "type": "text"
    },
    "imageUrl": {
      "type": "text"
    },
    "altText": {
      "type": "text"
    },
    "includedInAll4Access": {
      "type": "boolean",
      "required": true
    },
    "month": {
      "type": "enumeration",
      "enum": [
        "January",
        "February",
        "March",
        "April",
        "May",
        "June",
        "July",
        "August",
        "September",
        "October",
        "November",
        "December"
      ]
    },
    "whereToWatchLink": {
      "type": "text"
    }
  }
}
